%tr
  %td= link_to name, edit_order_path(model)
  %td= created_at
  %td
    = form_tag order_path(model), method: :delete, 'data-index': model.id, class: 'swal-form-js display-none' do
      .clearfix
    %i.fa.fa-trash.swal{ 'data-index': model.id, 'data-placement': 'left', 'data-toggle': 'delete-subscription-tooltip', title: I18n.t('action.destroy') }
